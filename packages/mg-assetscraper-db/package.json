{
  "name": "@tryghost/mg-assetscraper-db",
  "version": "0.2.8",
  "repository": "https://github.com/TryGhost/migrate/tree/main/packages/mg-assetscraper-db",
  "author": "Ghost Foundation",
  "license": "MIT",
  "type": "module",
  "main": "build/index.js",
  "types": "build/types.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./build/index.js"
    }
  },
  "scripts": {
    "dev": "echo \"Implement me!\"",
    "build:watch": "tsc --watch --preserveWatchOutput --sourceMap",
    "build": "rm -rf build && rm -rf tsconfig.tsbuildinfo && tsc --build --sourceMap",
    "prepare": "yarn build",
    "lint": "eslint src/ --ext .ts --cache",
    "posttest": "yarn lint",
    "test": "rm -rf build && yarn build --force && NODE_ENV=test c8 --src src --all --check-coverage --100 --reporter text --reporter cobertura node --test --test-concurrency=1 'build/test/**/*.test.js'"
  },
  "files": [
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/sinon": "21.0.0",
    "@typescript-eslint/eslint-plugin": "8.56.1",
    "@typescript-eslint/parser": "8.56.1",
    "c8": "11.0.0",
    "dotenv": "17.3.1",
    "eslint": "10.0.2",
    "nock": "14.0.11",
    "sinon": "21.0.1",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@tryghost/debug": "1.2.1",
    "@tryghost/errors": "2.2.1",
    "@tryghost/listr-smart-renderer": "0.5.15",
    "@tryghost/mg-fs-utils": "0.16.5",
    "@tryghost/string": "0.3.1",
    "file-type": "21.3.0",
    "fs-extra": "11.3.3",
    "heic-convert": "2.1.0",
    "sequelize": "6.37.7",
    "sharp": "0.34.5",
    "sqlite3": "5.1.7",
    "transliteration": "2.6.1",
    "uuid": "13.0.0"
  }
}
